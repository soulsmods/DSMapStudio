<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="3">
  <ParamType>ARENA_PARAM_ST</ParamType>
  <DataVersion>1</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>203</FormatVersion>
  <Fields>
    <Field Def="s32 missionParamId">
      <Description>MissionParam row ID; mainly determines which map to fight on.</Description>
    </Field>
    <Field Def="s8 menuCategory">
      <Description>Value determining which tab in the arena menu to use; can be set arbitrarily. Tab text FMG ID is 258010 + menuCategory in FNR_メニューテキスト.</Description>
    </Field>
    <Field Def="dummy8 pad08[3]"></Field>
    <Field Def="s32 UnkInt0x08"></Field>
    <Field Def="s32 UnkInt0x0C"></Field>
    <Field Def="s32 accountParamId">
      <Description>AccountParam row ID; opponent's name and emblem.</Description>
    </Field>
    <Field Def="s32 charaInitParamId">
      <Description>CharaInitParam row ID; opponent's loadout and customization.</Description>
    </Field>
    <Field Def="s32 npcParamId">
      <Description>NpcParam row ID; opponent's stats.</Description>
    </Field>
    <Field Def="s32 npcThinkParamId">
      <Description>NpcThinkParam row ID; opponent's AI.</Description>
    </Field>
    <Field Def="s32 initialCoamReward">
      <Description>Money granted on first completion.</Description>
    </Field>
    <Field Def="dummy8 pad24[12]"></Field>
    <Field Def="s32 repeatCoamReward">
      <Description>Money granted on repeat completions.</Description>
    </Field>
    <Field Def="dummy8 pad34[12]"></Field>
    <Field Def="s32 introCutsceneId">
      <Description>Cutscene ID to play at the start of the mission.</Description>
    </Field>
    <Field Def="s32 bgmSoundId">
      <Description>Background music to play during the mission.</Description>
    </Field>
    <Field Def="s32 rankTextureId">
      <Description>Texture ID defined in the SB_ArenaRank layout.</Description>
    </Field>
    <Field Def="s32 outroCutsceneId">
      <Description>Cutscene ID to play at the end of the mission.</Description>
    </Field>
    <Field Def="dummy8 pad50[16]"></Field>
  </Fields>
</PARAMDEF>