<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="3">
  <ParamType>RAGDOLL_PARAM_ST</ParamType>
  <DataVersion>1</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>202</FormatVersion>
  <Fields>
    <Field Def="f32 hierarchyGain = 0.17">
      <DisplayName>ヒエラルキーゲイン</DisplayName>
      <Description>低くすると元のポーズに近づき、大きくするとぐにゃぐにゃになる。</Description>
      <Minimum>0</Minimum>
      <Maximum>1</Maximum>
      <SortID>400</SortID>
    </Field>
    <Field Def="f32 velocityDamping">
      <DisplayName>速度減衰</DisplayName>
      <Description>ラグドールの移動スピードの減速率。0に近づくとゆっくり戻り、1に近づくとぱっと戻る</Description>
      <Minimum>0</Minimum>
      <Maximum>1</Maximum>
      <SortID>500</SortID>
    </Field>
    <Field Def="f32 accelGain = 1">
      <DisplayName>加速度ゲイン</DisplayName>
      <Description>リジッドの加速度の調整パラメータ。低くすると動きが柔らかくなり、高くすると硬くなる。加速度ゲインよりも低い値にするらしい。</Description>
      <Minimum>0</Minimum>
      <Maximum>1</Maximum>
      <SortID>600</SortID>
    </Field>
    <Field Def="f32 velocityGain = 0.6">
      <DisplayName>速度ゲイン</DisplayName>
      <Description>リジッドの速度の調整パラメータ。低くすると動きが柔らかくなり、高くすると硬くなる。速度ゲインよりも低い値にするらしい。</Description>
      <Minimum>0</Minimum>
      <Maximum>1</Maximum>
      <SortID>700</SortID>
    </Field>
    <Field Def="f32 positionGain = 0.05">
      <DisplayName>位置ゲイン</DisplayName>
      <Description>リジッドの移動位置の調整パラメータ。低くすると動きが柔らかくなり、高くすると硬くなる。</Description>
      <Minimum>0</Minimum>
      <Maximum>1</Maximum>
      <SortID>800</SortID>
    </Field>
    <Field Def="f32 maxLinerVelocity = 1.4">
      <DisplayName>最大速度</DisplayName>
      <Description>リジッドの最大移動速度</Description>
      <Minimum>0</Minimum>
      <Maximum>10</Maximum>
      <SortID>900</SortID>
    </Field>
    <Field Def="f32 maxAngularVelocity = 1.8">
      <DisplayName>最大角速度</DisplayName>
      <Description>リジッドの最大角速度</Description>
      <Minimum>0</Minimum>
      <Maximum>10</Maximum>
      <SortID>1000</SortID>
    </Field>
    <Field Def="f32 snapGain = 0.1">
      <DisplayName>スナップゲイン</DisplayName>
      <Description>元のポーズに近づけるための調整値。位置ゲインと似た効果</Description>
      <Minimum>0</Minimum>
      <Maximum>1</Maximum>
      <SortID>1100</SortID>
    </Field>
    <Field Def="u8 enable">
      <DisplayName>動くか</DisplayName>
      <Enum>RAGDOLL_PARAM_BOOL</Enum>
      <Description>ダメージラグドール時に動くか</Description>
      <Maximum>1</Maximum>
      <SortID>100</SortID>
    </Field>
    <Field Def="s8 partsHitMaskNo = -1">
      <DisplayName>部位あたりマスク番号</DisplayName>
      <Description>部位あたりマスク番号。-1:マスク無効</Description>
      <Minimum>-1</Minimum>
      <Maximum>31</Maximum>
      <SortID>200</SortID>
    </Field>
    <Field Def="dummy8 pad[14]">
      <DisplayName>パディング</DisplayName>
      <Description>パディング</Description>
      <DisplayFormat>%d</DisplayFormat>
      <EditFlags>Wrap</EditFlags>
      <Maximum>1</Maximum>
      <Increment>1</Increment>
      <SortID>99999999</SortID>
    </Field>
  </Fields>
</PARAMDEF>