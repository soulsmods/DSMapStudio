      <Minimum>-1</Minimum>
      <Maximum>1E+09</Maximum>
      <SortID>50000</SortID>
    </Field>
    <Field Def="s32 spEffectId3 = -1">
      <DisplayName>特殊効果ID3</DisplayName>
