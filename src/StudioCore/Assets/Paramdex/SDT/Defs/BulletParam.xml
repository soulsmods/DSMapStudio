      <Minimum>-1</Minimum>
      <Maximum>1E+09</Maximum>
      <SortID>51000</SortID>
    </Field>
    <Field Def="s32 spEffectId4 = -1">
      <DisplayName>特殊効果ID4</DisplayName>
